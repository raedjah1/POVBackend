{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "povstreaming",
        "image": "637423613384.dkr.ecr.us-east-2.amazonaws.com/povstreaming:latest",
        "essential": true,
        "memory": 512,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          },
          {
            "hostPort": 1935,
            "containerPort": 1935
          }
        ],
        "mountPoints": [
          {
            "sourceVolume": "app",
            "containerPath": "/usr/src/app"
          }
        ]
      }
    ],
    "volumes": [
      {
        "name": "app",
        "host": {
          "sourcePath": "/var/app/current"
        }
      }
    ]
  }
  