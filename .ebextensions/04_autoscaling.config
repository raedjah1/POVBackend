option_settings:
  aws:autoscaling:asg:
    MinSize: 1
    MaxSize: 5
  aws:autoscaling:trigger:
    BreachDuration: 300
    LowerBreachScaleIncrement: -1
    LowerThreshold: 20
    MeasureName: CPUUtilization
    Period: 300
    Statistic: Average
    Unit: Percent
    UpperBreachScaleIncrement: 1
    UpperThreshold: 75